{
  "rules": [
    {
      "id": "001",
      "name": "1号规则",
      "protocol": "http",
      "serviceId": "backend-http-server",
      "prefix": "/user",
      "paths": [
        "/http-server/ping",
        "/http-server/test"
      ],
      "filterConfigs": [
        {
          "id": "load_balance_filter",
          "config": {
            "load_balance": "RoundRobin"
          }
        },
        {
          "id": "flow_ctl_filter"
        }
      ],
      "flowCtlConfigs": [
        {
          "type": "path",
          "model": "distributed_flowCtl",
          "value": "/http-server/ping",
          "config": {
            "duration": 20,
            "permits": 2
          }
        }
      ]
    }
  ]
}